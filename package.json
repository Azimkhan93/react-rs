{
  "name": "react-next-js-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "prepare": "husky install",
    "prettier": "prettier --check src",
    "format:fix": "prettier --write src",
    "test": "jest",
    "coverage": "jest --coverage"
